# Importing necessary libraries for clustering
from sklearn.cluster import KMeans
from sklearn.preprocessing import StandardScaler

# Preparing the data for clustering
# Selecting relevant features for clustering
clustering_data = summary[
                    ['CLV', 'recency', 'frequency', 'monetary_value'
                    ]
                ].copy()

# Standardizing the data
scaler = StandardScaler()
clustering_data_scaled = scaler.fit_transform(clustering_data)

# Implementing KMeans clustering
kmeans = KMeans(n_clusters=3, random_state=42)
summary['cluster'
                ] = kmeans.fit_predict(clustering_data_scaled)

# Visualizing the clusters
plt.figure(figsize=(10,
                6))
sns.scatterplot(data=summary, x='recency', y='frequency', hue='cluster', palette='viridis', alpha=0.6)
plt.title('Customer Segmentation using KMeans Clustering')
plt.xlabel('Recency')
plt.ylabel('Frequency')
plt.legend(title='Cluster')
plt.show()
